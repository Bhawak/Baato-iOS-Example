  # Pods for baato-navigation
  pod 'Mapbox-iOS-SDK', '~> 5.2'
  pod 'MapboxNavigation', '~> 0.37.0'

  target 'Baato iOS ExampleTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Baato iOS ExampleUITests' do
    # Pods for testing
  end
end
